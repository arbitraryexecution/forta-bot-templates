{
  "name": "forta-{developerAbbreviation}-{protocolAbbreviation}-account-balance",
  "version": "0.0.2",
  "description": "This agent template monitors the account balance (in Ether) of specific addresses.",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "nodemon --watch src --watch forta.config.json -e js,json --exec 'forta-agent run'",
    "start:prod": "forta-agent run --prod",
    "tx": "forta-agent run --tx",
    "block": "forta-agent run --block",
    "range": "forta-agent run --range",
    "file": "forta-agent run --file",
    "publish": "forta-agent publish",
    "disable": "forta-agent disable",
    "enable": "forta-agent enable",
    "keyfile": "forta-agent keyfile",
    "test": "jest"
  },
  "dependencies": {
    "bignumber.js": "^9.0.2",
    "forta-agent": "^0.0.41"
  },
  "devDependencies": {
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.2.5",
    "nodemon": "^2.0.8"
  }
}
